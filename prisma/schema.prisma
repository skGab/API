generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model tasks {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  v         Int     @map("__v")
  complete  Boolean
  text      String
  timestamp String
  userID    String
}

model userinfos {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  v        Int     @map("__v")
  email    String?
  password String
  user     String?
}
